{
  "name": "dev-challenge-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "start": "node build/index.js",
    "start:watch": "nodemon build/index.js -w build/",
    "test": "ava --tap",
    "types": "graphql-codegen --config codegen.yml",
    "types:watch": "nodemon --exec \"npm run types\" -e graphql --watch",
    "dev": "concurrently npm:start:watch npm:build:watch npm:types:watch"
  },
  "devDependencies": {
    "@types/node": "^12.7.1",
    "@types/tough-cookie": "^2.3.5",
    "ava": "^2.2.0",
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1",
    "tough-cookie": "^3.0.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^1.5.0",
    "@graphql-codegen/typescript": "^1.5.0",
    "@graphql-codegen/typescript-resolvers": "^1.5.0",
    "@types/express-session": "^1.15.13",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "graphql": "^14.4.2",
    "graphql-request": "^1.8.2",
    "graphql-yoga": "^1.18.1"
  }
}
