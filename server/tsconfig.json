{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build",
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext",
    "typeRoots": ["./node_modules/@types", "./typings"]
  }
}
